{
  "default_module": "powerline.segments.common",
  "segments": {
    "left": [ {
      "name": "hostname",
      "args": {
        "exclude_domain": true
      }
    }, {
      "type": "string",
      "contents": "master",
      "highlight_group": "session"
    } ],
    "right": [ {
      "module": "airport",
      "name": "format",
      "args": {
        "format": "%(state)s%(ssid)s",
        "running_text": "📶  ",
        "off_text": "📴 ",
        "init_text": "❔ "
      }
    }, {
      "module": "battery",
      "name": "format",
      "args": {
        "format": "%(current_source)s %(current_percent)d%%%(formatted_time_remaining)s",
        "time_format": " ~%(fuzzy_time_remaining)s",
        "AC_text": "🔌 ",
        "battery_text": "🔋 "
      }
    }, {
      "name": "date",
      "args": {
        "format": "%F"
      }
    }, {
      "name": "date",
      "args": {
        "format": "%R",
        "istime": true
      }
    }, {
      "name": "system_load",
      "args": {
        "format": "▴"
      }
    } ]
  }
}
